<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".content.coroutine.CoroutineActivity">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/tv_ex1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ex01"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:theme="@style/Widget.MaterialComponents.Button"
            app:strokeColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            style="?attr/materialButtonOutlinedStyle" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/tv_ex2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ex02"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:theme="@style/Widget.MaterialComponents.Button"
            app:strokeColor="@color/black"
            app:layout_constraintTop_toBottomOf="@id/tv_ex1"
            app:layout_constraintStart_toStartOf="parent"
            style="?attr/materialButtonOutlinedStyle" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/tv_ex3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ex03"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:theme="@style/Widget.MaterialComponents.Button"
            app:strokeColor="@color/black"
            app:layout_constraintTop_toBottomOf="@id/tv_ex2"
            app:layout_constraintStart_toStartOf="parent"
            style="?attr/materialButtonOutlinedStyle" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/tv_ex4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ex04"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:theme="@style/Widget.MaterialComponents.Button"
            app:strokeColor="@color/black"
            app:layout_constraintTop_toBottomOf="@id/tv_ex3"
            app:layout_constraintStart_toStartOf="parent"
            style="?attr/materialButtonOutlinedStyle" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>